{
	"name": "jsx-smart-copy",
	"displayName": "JSX Smart Copy Paste",
	"description": "Copy JSX elements with their imports",
	"version": "1.0.0",
	"publisher": "theiliad",
	"author": {
		"name": "theiliad"
	},
	"engines": {
		"vscode": "^1.97.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"jsx",
		"react",
		"copy",
		"paste",
		"import"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/theiliad/vscode-jsx-smart-copy"
	},
	"bugs": {
		"url": "https://github.com/theiliad/vscode-jsx-smart-copy/issues"
	},
	"license": "MIT",
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jsx-smart-copy.copy",
				"title": "Smart Copy JSX with Imports"
			},
			{
				"command": "jsx-smart-copy.paste",
				"title": "Smart Paste JSX with Imports"
			}
		],
		"keybindings": [
			{
				"command": "jsx-smart-copy.copy",
				"key": "cmd+ctrl+c",
				"mac": "cmd+ctrl+c",
				"win": "ctrl+alt+c",
				"linux": "ctrl+alt+c",
				"when": "editorTextFocus && (editorLangId == javascriptreact || editorLangId == typescriptreact)"
			},
			{
				"command": "jsx-smart-copy.paste",
				"key": "cmd+ctrl+v",
				"mac": "cmd+ctrl+v",
				"win": "ctrl+alt+v",
				"linux": "ctrl+alt+v",
				"when": "editorTextFocus && (editorLangId == javascriptreact || editorLangId == typescriptreact)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@stylistic/eslint-plugin": "^4.2.0",
		"@types/node": "20.x",
		"@types/vscode": "^1.97.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"eslint": "^8.57.0",
		"typescript": "^5.4.5",
		"typescript-eslint": "^8.32.1"
	}
}
